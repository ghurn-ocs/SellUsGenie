# Production Environment Configuration for GoDaddy Hosting
# Copy this to .env.production and update with your production values

# Supabase Configuration (Production)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_production_supabase_anon_key

# Stripe Configuration (Production)
VITE_STRIPE_PUBLISHABLE_KEY=pk_live_your_stripe_publishable_key
STRIPE_SECRET_KEY=sk_live_your_stripe_secret_key

# Google OAuth (Production)
VITE_GOOGLE_CLIENT_ID=your_production_google_client_id

# Google Maps API (Production)
VITE_GOOGLE_MAPS_API_KEY=your_production_google_maps_api_key

# Apple OAuth (Production)
VITE_APPLE_CLIENT_ID=com.sellusgenie.web.auth

# Application Configuration (Production)
VITE_APP_URL=https://your-domain.com
VITE_API_URL=https://your-domain.com/api
NEXT_PUBLIC_APP_URL=https://your-domain.com

# Trial Configuration
VITE_TRIAL_DAYS=14

# Performance & Analytics
VITE_ENVIRONMENT=production
VITE_DEBUG_MODE=false
VITE_ANALYTICS_ENABLED=true

# Security
VITE_SECURE_COOKIES=true
VITE_FORCE_HTTPS=true