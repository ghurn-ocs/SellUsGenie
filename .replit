modules = ["nodejs-20", "web"]

hidden = [".config", "package-lock.json"]

[env]
NODE_ENV = "production"

[gitHubImport]
requiredFiles = [".replit"]

[nix]
channel = "stable-22_11"

[deployment]
deploymentTarget = "static"
publicDir = "dist"
build = ["npm", "install", "&&", "npm", "run", "build:quick"]

[[ports]]
localPort = 80
externalPort = 80